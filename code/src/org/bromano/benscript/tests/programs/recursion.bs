func fib(n) {
    if ( n <= 1)
        return n;
    return fib(n - 1) + fib(n - 2);
}


// Lambda implementation
var a = (n) => {
    if ( n <= 1)
        return n;
    return fib(n - 1) + fib(n - 2);
}

//First class functions
var fib = a;

console.println(fib(1));
console.println(fib(2));
console.println(fib(3));
console.println(fib(4));
